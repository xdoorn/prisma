<UserControl x:Class="Prisma.Warehouse.Views.BrowserDetailsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prisma.Warehouse.Views"
             xmlns:prism="http://prismlibrary.com/"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             prism:ViewModelLocator.AutoWireViewModel="True">
  <Grid>
    <StackPanel DataContext="{Binding BrowsableObject}">
      <TextBlock Text="{Binding DisplayName}" Margin="5" />
      <TextBlock Text="{Binding FilePath}" Margin="5" />
    </StackPanel>
  </Grid>
</UserControl>
